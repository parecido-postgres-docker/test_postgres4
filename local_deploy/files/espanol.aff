altstringchar   "\\'{E}"       É
altstringchar   "\\'{I}"       Í
altstringchar   "\\'{O}"       Ó
altstringchar   "\\'{U}"       Ú
altstringchar   "\\~{n}"       ñ
altstringchar   "\\~{N}"       Ñ
altstringchar   "\\\"{u}"      ü
altstringchar   "\\\"{U}"      Ü

# html encoding
altstringtype "html" "tex" ".html"

altstringchar   "\&aacute;"    á
altstringchar   "\&eacute;"    é
altstringchar   "\&iacute;"    í
altstringchar   "\&oacute;"    ó
altstringchar   "\&uacute;"    ú
