altstringchar   "\&Iacute;"    Í
altstringchar   "\&Oacute;"    Ó
altstringchar   "\&Uacute;"    Ú
altstringchar   "\&ntilde;"    ñ
altstringchar   "\&Ntilde;"    Ñ
altstringchar   "\&uuml;"      ü
altstringchar   "\&Uuml;"      Ü

# msdos encoding
altstringtype "msdos" "tex" ".txt"

altstringchar   \xa0   á
altstringchar   \xb5   Á
altstringchar   \x82   é
altstringchar   \x90   É
altstringchar   \xa1   í
