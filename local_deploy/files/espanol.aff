suffixes

flag *J:	# isimo
    E	> -E, ÍSIMO	# grande grandísimo
    E	> -E, ÍSIMOS	# grande grandísimos
    E	> -E, ÍSIMA	# grande grandísima
    E	> -E, ÍSIMAS	# grande grandísimas
    O	> -O, ÍSIMO	# tonto tontísimo
    O	> -O, ÍSIMA	# tonto tontísima
    O	> -O, ÍSIMOS	# tonto tontísimos
    O	> -O, ÍSIMAS	# tonto tontísimas
    L	> ÍSIMO	# formal formalísimo
    L	> ÍSIMA	# formal formalísima
    L	> ÍSIMOS	# formal formalísimos
    L	> ÍSIMAS	# formal formalísimas

flag *M:	# Mente
    O		> -O, AMENTE	# Como en claro > claramente
    [ELNRSZ]	> MENTE		# Como en virtual > virtualmente

flag *N:	# Mente. Verbos que admiten en su participio
		# la terminación mente
    [AI] R	> -R, DAMENTE	# amar > amadamente
    E R 	> -ER, IDAMENTE	# comer > comidamente

flag *P:	# Género y número
    E		> -E, A			# presidente presidenta
    O		> -O, A			# amigo amiga
    [EO]	> S			# amigo amiga
    O		> -O, AS		# amiga amigas
    E		> -E, AS		# presidente presidentas
    Z		> -Z, CES		# juez jueces
    Á N	> -ÁN,ANA		# charlatán charlatana
    Á N	> -ÁN,ANES		# charlatán charlatanes
    Á N	> -ÁN,ANAS		# charlatán charlatanas
    Í N	> -ÍN,INA		# mallorquín mallorquina
    Í N	> -ÍN,INES		# mallorquín mallorquines
    Í N	> -ÍN,INAS		# mallorquín mallorquinas
    Ó N	> -ÓN,ONA		# charlatán anglosajona
    Ó N	> -ÓN,ONES		# anglosajón anglosajona
    Ó N	> -ÓN,ONAS		# anglosajón anglosajona
    [^AEONS]	> A			# espectador espectadora
    [^AEONSZ]	> ES			# espectador espectadores
    [^AEONS]	> AS			# espectador espectadoras
    É S	> -ÉS,ESA		# aragonés aragonesa
    É S	> -ÉS,ESES		# aragonés aragoneses
    É S	> -ÉS,ESAS		# aragonés aragonesas

flag *H:	# Sustantivos acabados en -ción derivados de verbos 
    [AI] R	> -R, CIÓN		# abolir abolición
    [AI] R	> -R, CIONES		# abolir aboliciones

flag *I:	# Sustantivos acabados en -iento derivados de verbos 
    [AI] R	> -R, MIENTO		# abandonamiento
    [AI] R	> -R, MIENTOS		#
    E R		> -ER, IMIENTO		# aborrecer aborrecimiento
    E R		> -ER, IMIENTOS		#

flag *L:	# Participio activo para verbos regulares
    A R		> -R, NTE		# amar amante amantes
    A R		> -R, NTES
    E R		> -ER, IENTE		# temer temiente temientes
    E R		> -ER, IENTES
    I R		> -R, ENTE		# vivir viviente vivientes
    I R		> -R, ENTES

flag *R:	# reflexivos
    [AEI] R	> ME			# amar amarme
    [AEI] R	> TE			# amar amarte
    [AEI] R	> SE			# amar amarse
    [AEI] R	> NOS			# amar amarse
    [AEI] R	> OS			# amar amarse
    A R		> -AR, ÁNDOME		# amar amándome
    A R		> -AR, ÁNDOTE		# amar amándote
    A R		> -AR, ÁNDOSE		# amar amándose
    A R		> -AR, ÁNDONOS		# amar amándonos
    A R		> -AR, ÁNDOOS		# amar amándoos
    E R		> -ER, IÉNDOME		# comer comiéndose
    E R		> -ER, IÉNDOTE		# comer comiéndote
